<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ict.mapper.TimeMapper">
<!-- mapper 태그 사이에 어떤 메세드에 어떤 쿼리문을 연결할지 작성, 쿼리문의 종류에 따라 사용하는 태그가 달라짐, 자료형은 소문자로 작성 -->
	<select id="getTime2" resultType="string">
	<!-- getTime2 메서드가 실행될 때 실행할 쿼리문(세미콜론 제외)을 태그 내부에 작성 -->
		SELECT SYSDATE FROM DUAL
	</select>
	<select id="getTime3" resultType="string">
		SELECT SYSDATE FROM DUAL
	</select>
</mapper>
